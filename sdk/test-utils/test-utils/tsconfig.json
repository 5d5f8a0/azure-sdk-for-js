{
  "extends": "../../../tsconfig.package",
  "compilerOptions": {
    "paths": {
      "@azure/test-utils": [
        "./src/index.ts"
      ]
    },
    "module": "NodeNext",
    "moduleResolution": "NodeNext",
    "rootDir": ".",
    "lib": [
      "dom",
      "esnext"
    ],
    "skipLibCheck": true
  },
  "exclude": [
    "node_modules",
  ],
  "include": [
    "./src/**/*.ts",
    "./test/**/*.ts"
  ]
}
